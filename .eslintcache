act(name, number)),\n});\n\nexport default connect(mapStateToProps, mapDispatchToProps)(ContactForm);\n","/home/nickname/Fullstack/goit-react-hw-07-phonebook/src/components/ContactList/ContactList.js",["108"],"import { connect } from 'react-redux';\nimport contactsOperations from '../../redux/contacts/contacts-operations';\nimport s from './ContactList.module.css';\n\nconst ContactList = ({ contacts, filter, deleteContact }) => (\n  <ul>\n    {contacts.map(\n      item =>\n        item.name.toLowerCase().includes(filter.toLowerCase()) && (\n          <li key={item.id}>\n            {item.name}: {item.number}{' '}\n            <button id={item.id} type=\"button\" onClick={deleteContact}>\n              Delete\n            </button>\n          </li>\n        ),\n    )}\n  </ul>\n);\n\nconst mapStateToProps = state => ({\n  contacts: state.contacts.contacts,\n  filter: state.contacts.filter,\n});\n\nconst mapDispatchToProps = dispatch => ({\n  deleteContact: e => dispatch(contactsOperations.deleteContact(e.target.id)),\n});\n\nexport default connect(mapStateToProps, mapDispatchToProps)(ContactList);\n","/home/nickname/Fullstack/goit-react-hw-07-phonebook/src/components/Filter/Filter.js",[],"/home/nickname/Fullstack/goit-react-hw-07-phonebook/src/redux/contacts/contacts-reducer.js",["109"],"/home/nickname/Fullstack/goit-react-hw-07-phonebook/src/redux/contacts/contacts-actions.js",[],"/home/nickname/Fullstack/goit-react-hw-07-phonebook/src/redux/contacts/contacts-operations.js",["110"],{"ruleId":"111","severity":1,"message":"112","line":4,"column":8,"nodeType":"113","messageId":"114","endLine":4,"endColumn":9},{"ruleId":"115","severity":1,"message":"116","line":35,"column":23,"nodeType":"117","messageId":"118","endLine":35,"endColumn":25},{"ruleId":"111","severity":1,"message":"112","line":3,"column":8,"nodeType":"113","messageId":"114","endLine":3,"endColumn":9},{"ruleId":"111","severity":1,"message":"119","line":1,"column":8,"nodeType":"113","messageId":"114","endLine